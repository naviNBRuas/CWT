# Environment variables for CWT
# Replace the placeholder with your actual main withdrawal address
CWT_MAIN_WITHDRAWAL_ADDRESS="your_main_crypto_address_here"

# Optional: override headless mode (True/False)
# CWT_HEADLESS_MODE=True

# Optional: per-exchange credentials examples
# CWT_EXCHANGE_A_USERNAME=
# CWT_EXCHANGE_A_PASSWORD=
# CWT_EXCHANGE_B_USERNAME=
# CWT_EXCHANGE_B_PASSWORD=
# CWT_BINANCE_EXCHANGE_USERNAME=
# CWT_BINANCE_EXCHANGE_PASSWORD=
# CWT_METAMASK_WALLET_PASSWORD=

# Optional: browser profile and chrome binary paths
# CWT_METAMASK_WALLET_BROWSER_PROFILE_PATH=
# CWT_CHROME_BINARY_LOCATION=

# Optional: notification credentials
# CWT_NOTIFICATIONS_SMTP_USERNAME=
# CWT_NOTIFICATIONS_SMTP_PASSWORD=
